<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.windhide.employee.mapper.ActivityMapper">
    <resultMap id="BaseResultMap" type="com.windhide.employee.pojo.Activity">
            <id property="activityId" column="activity_id" jdbcType="INTEGER"/>
            <result property="activityName" column="activity_name" jdbcType="VARCHAR"/>
            <result property="activityDiscount" column="activity_discount" jdbcType="DECIMAL"/>
            <result property="activityStartTime" column="activity_start_time" jdbcType="TIMESTAMP"/>
            <result property="activityEndTime" column="activity_end_time" jdbcType="TIMESTAMP"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        activity_id,activity_name,activity_discount,
        activity_start_time,activity_end_time,create_time
    </sql>
</mapper>
